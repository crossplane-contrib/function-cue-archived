// +kubebuilder:printcolumn:name="SYNCED",type="string",JSONPath=".status.conditions[?(@.type=='Synced')].status"
// +kubebuilder:printcolumn:name="EXTERNAL-NAME",type="string",JSONPath=".metadata.annotations.crossplane\\.io/external-name"
// +kubebuilder:printcolumn:name="AGE",type="date",JSONPath=".metadata.creationTimestamp"
// +kubebuilder:subresource:status
// +kubebuilder:resource:scope=Cluster,categories={crossplane,managed,gcp}
#V2Service: {
	// +kubebuilder:validation:XValidation:rule="self.managementPolicy == 'ObserveOnly' || has(self.forProvider.template)",message="template is a required parameter"
	spec:    #V2ServiceSpec   @go(Spec)
	status?: #V2ServiceStatus @go(Status)
}

// V2ServiceList contains a list of V2Services
#V2ServiceList: {
	items: [...#V2Service] @go(Items,[]V2Service)
}
